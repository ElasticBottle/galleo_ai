{
  "name": "www",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm with-env-local vite --port 6969",
    "build:dev": "pnpm with-env-dev vite build && tsc",
    "build:prod": "pnpm with-env-prod vite build && tsc",
    "serve:dev": "pnpm with-env-dev vite preview",
    "serve:prod": "pnpm with-env-prod vite preview",
    "test": "vitest run",
    "clean": "git clean -xdf .turbo node_modules dist .cache",
    "format": "bun x @biomejs/biome format . --write",
    "lint": "bun x @biomejs/biome lint . --write",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env-local": "dotenvx run -f ../../.env.local  -f ../../.env -- ",
    "with-env-dev": "dotenvx run  -f ../../.env -- ",
    "with-env-prod": "dotenvx run -f ../../.env.production -- "
  },
  "dependencies": {
    "@ai-sdk/react": "^1.2.11",
    "@galleo/backend": "workspace:*",
    "@galleo/env": "workspace:*",
    "@galleo/ui": "workspace:*",
    "@rectangular-labs/result": "0.0.1",
    "@tanstack/react-query": "^5.74.3",
    "@tanstack/react-query-devtools": "^5.74.3",
    "@tanstack/react-router": "^1.116.0",
    "@tanstack/react-router-devtools": "^1.116.0",
    "@tanstack/router-plugin": "^1.116.1",
    "hono": "^4.7.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@galleo/typescript": "workspace:*",
    "@tailwindcss/vite": "^4.1.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.1.0",
    "sass-embedded": "^1.87.0",
    "typescript": "^5.8.3",
    "vite": "^6.2.6",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-plugin-top-level-await": "^1.5.0",
    "vite-plugin-wasm": "^3.4.1",
    "vitest": "^3.1.1",
    "web-vitals": "^4.2.4"
  }
}
